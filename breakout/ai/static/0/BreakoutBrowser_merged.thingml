			property pro_game_nextLevel_val21 : Byte
			property received_pro_game_nextLevel_ : Boolean
			property received_pro_game_nextLevelval21 : Boolean
			on entry do
				clock!timer_start(0, 0xAB, period, 0xF9)
			end
			internal
			event t : clock?timer_timeout
			guard t.id == 0
			action do
				bx = bx + dx
				by = by + dy
				var wl : Int16 = LEFT + br
				var wr : Int16 = RIGHT - br
				var wt : Int16 = TOP + br
				var wb : Int16 = BOTTOM - br
				if (bx < wl) do
					do
						sound!tone_(0x27)
						sound!tonetimeval22freq(0x13, 0x21, tone_duration, tone2)
					end
					dx = -dx
					bx = 2 * wl - bx
					incrementScore(-1)
				end
				else if (bx > wr) do
					do
						sound!tone_(0x7D)
						sound!tonetimeval22freq(0x9B, 0x3E, tone_duration, tone2)
					end
					dx = -dx
					bx = 2 * wr - bx
					incrementScore(-1)
				end
				if (by < wt) do
					do
						sound!tonetimeval22freq(0xFD, 0x34, tone_duration, tone2)
						sound!tone_(0x2D)
					end
					dy = -dy
					by = 2 * wt - by
					incrementScore(-1)
				end
				else if (by > wb) do
					do
						game!lostBall_(0xFA)
						game!lostBallval20(0x64, 0x07)
					end
					do
						req_game!lostBall_(0x27)
						req_game!lostBallval20(0xD8, 0xB5)
					end
				end
				if (dy > 0) do
					if (by > pady - br and by < pady + br) do
						if (bx > padx - padlen / 2 and bx < padx + padlen / 2) do
							do
								sound!tone_(0xEE)
								sound!tonetimeval22freq(0x47, 0x37, tone_duration, tone2)
							end
							dy = -dy
							by = 2 * (pady - br) - by
							dx = dx / 4 + (bx - padx) / 4
						end
					end
				end
				var collision : Boolean = collideBrick(bx - br, by - br)or collideBrick(bx + br, by - br)or collideBrick(bx + br, by + br)or collideBrick(bx - br, by + br)
				if (collision) do
					do
						sound!tone_(0x75)
						sound!tonetimeval22freq(0x1D, 0x68, tone_duration, tone1)
					end
					dy = -dy
					incrementScore(10)
					if (bricksLeft()== 0) do
						do
							game!nextLevel_(0xDB)
							game!nextLevelval21(0xB0, 0x6E)
						end
						do
							req_game!nextLevel_(0x18)
							req_game!nextLevelval21(0x59, 0xF3)
						end
					end
				end
				drawBall()
				drawPad()
				do
					ia!updateIAballypadyballxpadxval17(0x3B, padx, bx, by, pady, 0x34)
