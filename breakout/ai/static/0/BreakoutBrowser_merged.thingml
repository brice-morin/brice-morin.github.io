	function incrementScore(diff : Int8) do
		score = score + diff as Int16
		if (score < 0) score = 0
		drawScore()
	end
	function drawScore() do
		do
			display!setColorbrval4(158, 0x8B, 0x40, 130)
			display!setColorg(0x60, 209)
		end
		do
			display!setBGColorb(fgcolor [2], 0xEB)
			display!setBGColorval5rg(fgcolor [1], fgcolor [0], 0x9C, 0x64)
		end
		do
			display!drawIntegervdigitsscaleval8y(0xB9, score, 2, 0x93, 5, 2)
			display!drawIntegerx(0x6A, 58)
		end
	end
	function drawLives() do
		do
			display!setColorbrval4(fgcolor [0], 0xF4, 0xC6, fgcolor [2])
			display!setColorg(0x0B, fgcolor [1])
		end
		do
			display!fillRectywidth(4, 0x3E, 24 + 6)
			display!fillRectheightxval7(6, 0xB6, 0x34, 124)
		end
		do
			display!setColorbrval4(183, 0x46, 0x2C, 111)
			display!setColorg(0x91, 199)
		end
		var i : UInt8 = 0
		while (i < lives) do
			do
				display!fillRectywidth(4, 0x71, 6)
				display!fillRectheightxval7(6, 0x74, 0x9F, 124 + (2 - i) * 12)
			end
			i = i + 1
		end
	end
